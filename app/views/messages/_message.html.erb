<div class="cont-<%= message.user.id %>">
  <div class="message-box msg-<%= message.user.id %> " >
    <div class=" d-flex align-items-center">
      <% unless message.room.is_private %>
        <h6 class="name"><u> <%= message.user.username %> </u></h6>
      <% end %>
    </div>
    <%= message.content %>
  </div>
</div>