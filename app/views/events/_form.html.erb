<%= form_with model: @event do |form| %>

    <%= form.label :event_name%> 
    <%= form.text_field :event_name%><br>
    <%= form.label :caption%> 
    <%= form.text_field :caption%><br>
    <%= form.label :images%> 
    <%= form.file_field :images, multiple: true%><br>

    <%= form.submit%>
<%end%>
 